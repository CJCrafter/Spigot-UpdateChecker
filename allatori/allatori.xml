<config>
    <input>
        <jar in="${project.artifactId}.jar" out="${project.artifactId}.jar"/>
    </input>

    <classpath>
        <jar name="C:\Users\mfnal\.m2\repository\org\spigotmc\spigot\1.17.1-R0.1-SNAPSHOT\spigot-1.17.1-R0.1-SNAPSHOT.jar" />
    </classpath>

    <keep-names>
        <class access="public"/>
        <method access="public"/>
        <field access="public"/>
    </keep-names>

    <property name="log-file" value="allatori.xml"/>
    <property name="string-encryption" value="disable" />
    <property name="local-variables-naming" value="keep-parameters"/>
    <property name="control-flow-obfuscation" value="enable"/>
    <property name="extensive-flow-obfuscation" value="disable"/>
    <property name="line-numbers" value="remove"/>
    <property name="generics" value="remove"/>
    <property name="inner-classes" value="remove"/>
    <property name="member-reorder" value="disable"/>
    <property name="finalize" value="enable"/>
    <property name="output-jar-compression-level" value="9"/>


</config>